<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC '-//Hibernate/Hibernate Mapping DTD 3.0//EN' 'http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd'>
<hibernate-mapping>
 <class name='com.gf.ny.src.msa' table='msa'>
 	<id name='id' type="int" column='id'><generator class='increment'/></id>
 	<property name='acknowledgmentCode' column='acknowledgmentcode'/>
	<property name='textMessage' column='textmessage'/>
	<property name='expectedSequenceNumber' column='expectedsequencenumber'/>
	<property name='delayedAcknowledgmentType' column='delayedacknowledgmenttype'/>
	<property name='errorCondition' column='errorcondition'/>
	<property name="segment" column='segment'/>
 	<property name="messageBodyId" column='messagebodyid'/>
 	<property type="int" name="route" column='route'/>
</class>
</hibernate-mapping>